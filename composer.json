{
    "name": "staudenmeir/eloquent-param-limit-fix",
    "description": "Laravel Eloquent eager loading fix for parameter limits of SQLite and SQL Server",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require-dev": {
        "illuminate/database": "^10.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\EloquentParamLimitFix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staudenmeir\\EloquentParamLimitFix\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
